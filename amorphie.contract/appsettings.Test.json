{
  "ConnectionStrings": {
    "DefaultConnection": "DB_CONNECTION"
  },
  "TemplateEngine": {
    "Url": "TemplateEngineUrlValue",
    "HtmlRenderEndpoint": "TemplateEngineHtmlRenderEndpointValue",
    "PdfRenderEndpoint": "TemplateEnginePdfRenderEndpointValue",
    "GetTemplateEndpoint": "TemplateEngineGetTemplateEndpointValue"
  },
  "minio": {
    "EndPoint": "EndPointValue",
    "AccessKey": "AccessKeyValue",
    "SecretKey": "SecretKeyValue",
    "BucketName": "BucketNameValue"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.ElasticSearch"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Debug"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "ElasticSearch_URL",
          "indexFormat": "amoprhie-contract-index-{0:yyyy.MM}",
          "emitEventFailure": "WriteToSelfLog",
          "autoRegisterTemplate": true,
          "connectionGlobalHeaders": "Authorization=Basic ElasticSearch_AuthToken"
        }
      }
    ]
  },
  "ElasticApm": {
    "SecretToken": "ElasticApm_SecretToken",
    "ServerUrl": "ElasticApm_URL",
    "ServiceName": "LagerthaApi",
    "Environment": "Test",
    "TransactionSampleRate": 1.0
  },
  "MetricsOptions": {
    "DefaultContextLabel": "amoprhie-contract",
    "Enabled": true
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.1,
    "IgnoredHttpStatusCodes": [],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true,
    "EnvironmentInfoEndpointEnabled": true
  }
}