{
  "ConnectionStrings": {
    "DefaultConnection": "DB_CONNECTION"
  },
  "TemplateEngine": {
    "Url": "TemplateEngineUrlValue",
    "HtmlRenderEndpoint": "TemplateEngineHtmlRenderEndpointValue",
    "PdfRenderEndpoint": "TemplateEnginePdfRenderEndpointValue",
    "GetTemplateEndpoint": "TemplateEngineGetTemplateEndpointValue"
  },
  "minio": {
    "EndPoint": "EndPointValue",
    "AccessKey": "AccessKeyValue",
    "SecretKey": "SecretKeyValue",
    "BucketName": "BucketNameValue"
  },
  "ElasticApm": {
    "SecretToken": "ElasticApm_SecretToken",
    "ServerUrl": "ElasticApm_URL",
    "ServiceName": "amorphie-contract",
    "Environment": "Dev",
    "TransactionIgnoreUlrs": "/health"
  },
  "MetricsOptions": {
    "DefaultContextLabel": "amorphie-contract",
    "Enabled": true
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.1,
    "IgnoredHttpStatusCodes": [],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true,
    "EnvironmentInfoEndpointEnabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware" : "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { 
        "Name": "File", 
        "Args": { 
          "path": "/logs/log-amorphie-contract-.json", 
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "rollingInterval": "Day" 
        } 
      }
    ]
  },
  "Apisix": {
    "BaseUrl": "https://nonprod-apisix.burgan.com.tr/ebanking/contract/",
    "DownloadEndpoint": "document/download"
  }
}